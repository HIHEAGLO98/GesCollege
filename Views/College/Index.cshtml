@model IEnumerable<GesCollege.Models.College>@{    ViewBag.Title = "Index";}<div class="jumbotron">    <h1>        <span class="glyphicon glyphicon-education" aria-hidden="true"> College</span>    </h1>    <p class="lead"></p></div><div class="row first">    <div class="col-lg-8">        <div class="col-lg-3">            <a href="@Url.Action("Create")">                <span class="glyphicon glyphicon-plus-sign" aria-hidden="true">Create New</span>            </a>        </div>        @*<div class="col-lg-3">            <a href="@Url.Action("")">                <span class="glyphicon glyphicon-save-file" aria-hidden="true">                    Report PDF                </span>            </a>        </div>*@    </div>    @* @using (Html.BeginForm("Index", "Etudiant", FormMethod.Get))            {               <div class="col-lg-4">                    Find by name: @Html.TextBox("SearchString", ViewBag.CurrentFilter as string)                    <input type="submit" value="Search" />                </div>        }*@</div><table class="table">    <tr>        <th>            @Html.DisplayNameFor(model => model.Nom)        </th>        <th>            @Html.DisplayNameFor(model => model.SiteWeb)        </th>        <th></th>    </tr>    @foreach (var item in Model)    {        <tr>            <td>                @Html.DisplayFor(modelItem => item.Nom)            </td>            <td>                @Html.DisplayFor(modelItem => item.SiteWeb)            </td>            <td>                <a href="College/Edit/@Html.DisplayFor(modelItem => item.Id)">                    <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>                </a> |                <a href="College/Details/@Html.DisplayFor(modelItem => item.Id)">                    <span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>                </a> |                <a href="College/Delete/@Html.DisplayFor(modelItem => item.Id)">                    <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>                </a>            </td>        </tr>    }</table>